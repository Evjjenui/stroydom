.component.component-popular__objects
  .content-wrapper
    .component__title
      h1
        | Наши объекты
    .objects-block
      each obj_name, index in ["Коттеджный поселок «Новый Петербург»", "п. Белоостров", "д. Грузино"]
        a.object-item(href="./object.html")
          span.photo
            img(src="assets/images/object_photo" + (index + 1) + ".jpg", alt="")
            span.hide-info
              span.object-info__sizes
                span.square
                  p 170 м2
                span.frame
                  p 170 м2
                span.floors
                  p 170 м2

          span.object-info
            p= obj_name

    span.show-all
      a(href="./projects.html")
        | Посмотреть
        br
        | все проекты
